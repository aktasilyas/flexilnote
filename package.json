{
  "name": "flexinote-ai",
  "version": "1.0.0",
  "description": "AI Student Note Taking App",
  "main": "index.js",
  "scripts": {
    "start": "npx serve .",
    "build": "node build.js",
    "app:sync": "npx cap sync",
    "app:open:android": "npx cap open android",
    "app:open:ios": "npx cap open ios",
    "clean:android": "cd android && ./gradlew clean && cd ..",
    "rebuild:android": "npm run build && npx cap sync android && npx cap run android"
  },
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@google/genai": "^1.34.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0"
  }
}
